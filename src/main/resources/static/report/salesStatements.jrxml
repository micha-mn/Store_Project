	<queryString>
		<![CDATA[SELECT s.id,
    s.item_id,
    i.item_code,
    b.name_en AS brand_name,
    i.description,
    i.selling_price-i.consignment_price AS profit,
    s.selling_date
   FROM sales s,
    items i,
    brand b
  WHERE s.item_id::integer = i.id 
  AND i.brand_id::integer = b.id 
  and s.selling_date between  to_timestamp($P{fromDate},'yyyy-MM-dd hh24:mi')  and  to_timestamp($P{toDate},'yyyy-MM-dd hh24:mi')  ;]]>
